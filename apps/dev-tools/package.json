{
  "name": "@qwery/dev-tools",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_ENV=production react-router build",
    "dev": "react-router dev",
    "start": "cross-env NODE_ENV=production react-router-serve ./build/server/index.js",
    "typecheck": "node -e \"const fs=require('node:fs'); const cp=require('node:child_process'); fs.rmSync('.react-router/types',{recursive:true,force:true,maxRetries:10,retryDelay:100}); const r=cp.spawnSync('react-router typegen',{shell:true,encoding:'utf8'}); process.stdout.write(r.stdout||''); process.stderr.write(r.stderr||''); if(r.status!==0 && !(String(r.stdout)+String(r.stderr)).includes('ENOTEMPTY')) process.exit(r.status||1);\" && tsc",
    "clean": "git clean -xdf .turbo node_modules",
    "format": "prettier --check \"**/*.{ts,tsx}\"",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:fix": "eslint --cache --cache-location ./node_modules/.cache/eslint --fix ."
  },
  "dependencies": {
    "@qwery/domain": "workspace:*",
    "@qwery/ui": "workspace:*",
    "@react-router/fs-routes": "7.9.5",
    "@react-router/node": "^7.9.5",
    "@react-router/serve": "^7.9.5",
    "@xyflow/react": "^12.9.3",
    "isbot": "^5.1.31",
    "next-themes": "0.4.6",
    "react": "catalog:",
    "react-dom": "catalog:",
    "react-router": "catalog:"
  },
  "devDependencies": {
    "@qwery/eslint-config": "workspace:*",
    "@qwery/prettier-config": "workspace:*",
    "@qwery/tailwind-config": "workspace:*",
    "@qwery/tsconfig": "workspace:*",
    "@react-router/dev": "^7.9.5",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@vitejs/plugin-react": "^5.1.0",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.0",
    "tailwindcss": "4.1.16",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "prettier": "@qwery/prettier-config"
}
