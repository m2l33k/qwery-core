{
  "datasources": [
    {
      "id": "clickhouse-node",
      "name": "ClickHouse (Node)",
      "description": "Connect to ClickHouse databases",
      "icon": "/extensions/clickhouse-node/ClickHouseLogo_White_FNL.svg",
      "schema": {
        "type": "object",
        "properties": {
          "connectionUrl": {
            "type": "string",
            "description": "ClickHouse connection URL (clickhouse://user:pass@host:port/database or http://host:port)",
            "format": "uri"
          }
        },
        "required": [
          "connectionUrl"
        ]
      },
      "packageName": "@qwery/extension-clickhouse-node",
      "drivers": [
        {
          "id": "clickhouse.node",
          "name": "ClickHouse (Node)",
          "description": "Node runtime ClickHouse driver",
          "runtime": "node"
        }
      ]
    },
    {
      "id": "clickhouse-web",
