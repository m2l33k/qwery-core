{
  "name": "@qwery/extension-duckdb",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/driver.js",
  "scripts": {
    "lint": "echo \"no lint configured\"",
    "test": "vitest run --coverage --passWithNoTests",
    "build": "tsc -p tsconfig.build.json"
  },
  "contributes": {
    "datasources": [
      {
        "id": "duckdb",
        "name": "DuckDB",
        "description": "Connect to DuckDB databases",
        "icon": "media/DuckDB_stack-darkmode.svg",
        "schema": {
          "type": "object",
          "properties": {
            "database": {
              "type": "string",
              "description": "Database path (use :memory: for in-memory database)",
              "default": ":memory:"
            }
          }
        },
        "drivers": [
          "duckdb.default"
        ]
      }
    ],
    "drivers": [
      {
        "id": "duckdb.default",
        "name": "DuckDB (Node)",
        "description": "Default Node runtime DuckDB driver",
        "runtime": "node",
        "entry": "./dist/driver.js"
      }
    ]
  },
  "dependencies": {
    "@duckdb/node-api": "1.4.2-r.1",
    "@qwery/extensions-sdk": "workspace:*",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@vitest/coverage-istanbul": "catalog:",
    "vitest": "catalog:"
  }
}
