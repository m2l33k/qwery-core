{
  "name": "@qwery/playground",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "clean": "git clean -xdf .turbo node_modules",
    "format": "prettier --check \"**/*.{ts,tsx}\"",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:fix": "eslint --cache --cache-location ./node_modules/.cache/eslint --fix .",
    "test": "vitest run --logHeapUsage --coverage --passWithNoTests",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "exports": {
    "./playground-try": "./src/components/playground-try.tsx",
    "./playgrounds": "./src/playgrounds.ts",
    "./factory": "./src/factory/playground-factory.ts",
    "./factory/database": "./src/factory/playground-database.ts"
  },
  "dependencies": {
    "@electric-sql/pglite": "^0.3.12",
    "@qwery/domain": "workspace:*",
    "@qwery/extensions-loader": "workspace:*",
    "@qwery/extensions-sdk": "workspace:*",
    "nanoid": "^5.1.6",
    "uuid": "catalog:"
  },
  "devDependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@qwery/eslint-config": "workspace:*",
    "@qwery/i18n": "workspace:*",
    "@qwery/prettier-config": "workspace:*",
    "@qwery/shared": "workspace:*",
    "@qwery/tailwind-config": "workspace:*",
    "@qwery/tsconfig": "workspace:*",
    "@qwery/ui": "workspace:*",
    "@radix-ui/react-icons": "^1.3.2",
    "@tanstack/react-query": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@vitest/coverage-istanbul": "catalog:",
    "@vitest/ui": "^4.0.7",
    "eslint": "^9.39.0",
    "fake-indexeddb": "^6.0.2",
    "jsdom": "^27.1.0",
    "lucide-react": "^0.552.0",
    "next-themes": "0.4.6",
    "react": "catalog:",
    "react-dom": "catalog:",
    "react-hook-form": "^7.66.0",
    "react-i18next": "^16.2.3",
    "sonner": "^2.0.7",
    "vitest": "catalog:",
    "zod": "catalog:"
  },
  "prettier": "@qwery/prettier-config",
  "typesVersions": {
    "*": {
      "*": [
        "src/*"
      ]
    }
  }
